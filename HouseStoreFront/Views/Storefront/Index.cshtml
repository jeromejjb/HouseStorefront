@model IEnumerable<House><a asp-action="AddHouse" asp-controller="Product">Add New House</a>@foreach (House h in Model){<div style="border: 1px solid black">    <p>Id: @h.Id</p>    <p>Address @h.Address</p>    <p>Price: @h.Price</p>    <p>Quantiity: @h.Quantity</p>    <p>Description: @h.Description</p>    <a asp-action="Buy" asp-controller="Storefront" asp-route-Id="@h.Id">Buy the @h.Address Property</a>    <br />    <a asp-action="Details" asp-controller="Product" asp-route-ID="@h.Id">View Details for the @h.Address listing</a>    <br />    <a asp-action="Update" asp-controller="Product" asp-route-Id="@h.Id">Update the @h.Address listing</a>    <br />    <a asp-action="Delete" asp-controller="Product" asp-route-Id="@h.Id">Delete the @h.Address listing</a>    </div>}