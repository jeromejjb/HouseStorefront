@model IEnumerable<House><a asp-action="AddHouse" asp-controller="Product">Add New House</a>    @foreach (House h in Model)    {<div style="border: 1px solid black">    <p>Id: @h.Id</p>    <h2>Address @h.Address</h2>    <p>Price: @h.Price</p>    <p>Quantity: @h.Quantity</p>    <p>Description: @h.Description</p>    <a asp-action="Buy" asp-controller="Storefront" asp-route-Id="@h.Id">Buy the @h.Address Property</a>    <br />    <br />    <a asp-action="Details" asp-controller="Product" asp-route-Id="@h.Id">Details on the the @h.Address Property</a>    <br />    <a asp-action="Update" asp-controller="Product" asp-route-Id="@h.Id">Update the @h.Address listing</a>    <br />    <a asp-action="Delete" asp-controller="Product" asp-route-Id="@h.Id">Delete the @h.Address listing</a>    <br /></div>}