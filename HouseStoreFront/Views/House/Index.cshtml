@model IEnumerable<House><a asp-action="AddHouse" asp-controller="Home">Add New House</a>    @foreach (House h in Model)    {    <div style="border: 1px solid black">        <p>Id: @h.Id</p>        <p>Address @h.Address</p>        <p>Price: @h.Price</p>        <p>Quantiity: @h.Quantity</p>        <p>Description: @h.Description</p>        <a asp-action="Buy" asp-controller="Home" asp-route-Id="@h.Id">Buy the @h.Address Property</a>        <br />    </div>}